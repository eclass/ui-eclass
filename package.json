{
  "name": "react-ui-eclass-v8",
  "version": "0.1.0",
  "description": "Componentes UI de eClass",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/eclass/ui-eclass"
  },
  "bugs": {
    "url": "https://github.com/eclass/ui-eclass/issues"
  },
  "homepage": "https://ui-eclass.netlify.com/",
  "keywords": [
    "ui",
    "eclass",
    "components",
    "react"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@testing-library/jest-dom": "4.1.2",
    "@testing-library/react": "9.3.0",
    "availity-reactstrap-validation": "2.6.0",
    "bootstrap": "4.3.1",
    "classnames": "2.2.6",
    "husky": "3.0.9",
    "lint-staged": "9.4.2",
    "polished": "3.4.1",
    "prop-types": "15.7.2",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.2.0",
    "react-touch-ripple": "0.2.1",
    "reactstrap": "8.0.1",
    "styled-components": "4.4.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "develop": "start-storybook -p 9009 -s public",
    "storybook": "build-storybook -c .storybook",
    "build-storybook": "build-storybook -s public",
    "lint:js": "eslint . --ext .js,.jsx",
    "lint:js:fix": "npm run lint:js -- --fix",
    "format": "prettier-standard '{src,test}/**/*.{js,tsx}'"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "npm run --silent lint:js:fix",
      "prettier-standard",
      "git add"
    ]
  },
  "prettier": {
    "jsxBracketSameLine": true,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": false
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "standard",
      "react-app",
      "prettier",
      "prettier/flowtype",
      "prettier/react",
      "prettier/standard"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    },
    "plugins": [
      "react",
      "flowtype",
      "prettier",
      "standard"
    ],
    "rules": {
      "no-console": [
        "error"
      ],
      "jsx-a11y/href-no-hash": [
        0
      ],
      "@typescript-eslint/consistent-type-assertions": "off",
      "@typescript-eslint/no-angle-bracket-type-assertion": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-a11y": "5.2.4",
    "@storybook/addon-actions": "5.2.4",
    "@storybook/addon-docs": "5.2.4",
    "@storybook/addon-links": "5.2.4",
    "@storybook/addon-viewport": "5.2.4",
    "@storybook/addons": "5.2.4",
    "@storybook/react": "5.2.4",
    "babel-eslint": "10.0.3",
    "eslint": "6.5.1",
    "eslint-config-prettier": "6.4.0",
    "eslint-config-react-app": "5.0.2",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-flowtype": "4.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.1.2",
    "eslint-plugin-standard": "4.0.1",
    "prettier": "1.18.2",
    "prettier-standard": "15.0.1",
    "semantic-release": "^15.13.24",
    "storybook-readme": "5.0.8"
  },
  "renovate": {
    "extends": [
      "@eclass:js-lib"
    ]
  }
}
